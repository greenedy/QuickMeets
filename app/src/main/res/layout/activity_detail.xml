<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.mark.mroz.quickmeets.DetailActivity">

<TextView
    android:id="@+id/textView19"
    android:layout_width="99dp"
    android:layout_height="34dp"
    android:gravity="left|center"
    android:text="TextView"
    android:textSize="20sp"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="419dp" />

<TextView
    android:id="@+id/players"
    android:layout_width="99dp"
    android:layout_height="34dp"
    android:gravity="left|center"
    android:text="Players"
    android:textSize="20sp"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="371dp" />

<TextView
    android:id="@+id/playervalue"
    android:layout_width="192dp"
    android:layout_height="34dp"
    android:gravity="left|center"
    android:text="Number of ppl"
    android:textSize="20sp"
    tools:layout_editor_absoluteX="172dp"
    tools:layout_editor_absoluteY="371dp" />

<TextView
    android:id="@+id/textView4"
    android:layout_width="123dp"
    android:layout_height="40dp"
    android:fontFamily="serif"
    android:gravity="center"
    android:text="Sport ID"
    android:textColor="?android:attr/actionMenuTextColor"
    android:textSize="20sp"
    tools:layout_editor_absoluteX="131dp"
    tools:layout_editor_absoluteY="262dp" />

<ImageView
    android:id="@+id/imageView3"
    android:layout_width="385dp"
    android:layout_height="478dp"
    app:srcCompat="@drawable/background_image"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="-164dp" />

<ImageView
    android:id="@+id/eventPic"
    android:layout_width="162dp"
    android:layout_height="159dp"
    app:srcCompat="@drawable/default_sporttype"
    tools:layout_editor_absoluteX="111dp"
    tools:layout_editor_absoluteY="103dp" />

<ImageView
    android:id="@+id/userPic"
    android:layout_width="80dp"
    android:layout_height="127dp"
    app:srcCompat="@drawable/anotherusericon"
    tools:layout_editor_absoluteX="7dp"
    tools:layout_editor_absoluteY="-3dp" />

<TextView
    android:id="@+id/userId"
    android:layout_width="123dp"
    android:layout_height="50dp"
    android:fontFamily="cursive"
    android:gravity="center"
    android:text="Qasim "
    android:textColor="?android:attr/colorMultiSelectHighlight"
    android:textSize="35sp"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="103dp" />

<TextView
    android:id="@+id/textView7"
    android:layout_width="99dp"
    android:layout_height="34dp"
    android:gravity="left|center"
    android:text="Intencity"
    android:textSize="20sp"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="325dp" />


<Button
    android:id="@+id/button3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Button"
    tools:layout_editor_absoluteX="123dp"
    tools:layout_editor_absoluteY="472dp" />

<RatingBar
    android:id="@+id/ratingBar2"
    android:layout_width="196dp"
    android:layout_height="53dp"
    android:numStars="5"
    tools:layout_editor_absoluteX="172dp"
    tools:layout_editor_absoluteY="318dp" />

<ImageView
    android:id="@+id/imageView6"
    android:layout_width="187dp"
    android:layout_height="43dp"
    app:srcCompat="@drawable/available_icon"
    tools:layout_editor_absoluteX="172dp"
    tools:layout_editor_absoluteY="410dp" />

</android.support.constraint.ConstraintLayout>

